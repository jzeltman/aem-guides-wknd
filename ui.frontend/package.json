{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "webpack-dev-server --open --config ./webpack.dev.js",
    "storybook": "start-storybook -p 6006",
    "sba": "sba"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "acorn": "^6.1.0",
    "aem-clientlib-generator": "^1.4.3",
    "autoprefixer": "^9.2.1",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.14.1",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.13.0",
    "tslint-loader": "^3.5.4",
    "tslint-webpack-plugin": "^2.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-import-glob-loader": "^1.6.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@storybook/aem": "0.0.9",
    "@storybook/aem-cli": "0.0.9",
    "animate.css": "^3.7.2",
    "jquery": "^3.4.1",
    "normalize-scss": "^7.0.1"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "@storybook/aem-cli": {
    "storybookLocation": ".storybook",
    "storybookStoryLocation": "./stories",
    "localPackagePath": ".storybook/storybook-library/",
    "appsPath": "../ui.apps",
    "componentPaths": [
      "../ui.apps/src/main/content/jcr_root/apps/wknd/components/content",
      "../ui.apps/src/main/content/jcr_root/apps/wknd/components/form",
      "../ui.apps/src/main/content/jcr_root/apps/wknd/components/structure"
    ],
    "storybookAEMStyleSystem": true,
    "storybookAEMGrid": false,
    "storybookAEMPageTemplate": false,
    "storybookAEMConfluence": false,
    "storybookAEMFoundation": false,
    "aemContentDefaultPageResourceType": "storybook/components/structure/page",
    "aemContentDefaultPageTemplate": "/conf/wknd/settings/wcm/templates/content-page-template",
    "aemContentDefaultPageContentPath": "/root/responsivegrid/responsivegrid",
    "aemStoryHeadingComponentResourceType": "wknd/components/content/title",
    "aemStoryHeadingComponentTitleProperty": "jcr:title",
    "storyRoot": "Components",
    "packageGroup": "storybook",
    "packageName": "storybook-library.zip",
    "aemContentPath": "/content/wknd/us/en/storybook",
    "createAEMContent": true
  }
}
